{
  "name": "@wandelbots/wandelbots-js",
  "type": "module",
  "version": "0.0.0-semantically-released",
  "description": "Official JS client for the Wandelbots API",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "tsc": "tsc --pretty --noEmit",
    "dev": "npm run build -- --watch",
    "build": "tsup src/index.ts --format esm,cjs --clean --dts --minify --sourcemap",
    "test": "vitest run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wandelbotsgmbh/wandelbots-js.git"
  },
  "keywords": [
    "wandelbots",
    "api"
  ],
  "author": "Wandelbots",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/three": "^0.166.0",
    "prettier-eslint": "^16.3.0",
    "semantic-release": "^24.0.0",
    "tsup": "^8.1.0",
    "typescript": "^5.5.3",
    "vitest": "^2.0.2"
  },
  "dependencies": {
    "@wandelbots/wandelbots-api-client": "^0.97.0",
    "mobx": "^6.12.4",
    "mobx-react-lite": "^4.0.7",
    "reconnecting-websocket": "^4.4.0",
    "three": "^0.166.1",
    "url-join": "^5.0.0"
  }
}
